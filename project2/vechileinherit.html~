<script>
    function vechile(MaxSpeed, Type, Displacement) {
        this.maximumspeed = MaxSpeed;
        this.typesofVechile = Type;
        this.Displacementofvech = Displacement;
    }
    vechile.prototype.quantity = 1;

    vechile.prototype.speed = 90;

    var x = prompt(" maximum  speed");
    var y = prompt(" Type of vec");
    var z = prompt("Type distance tarveled");
    var Bike = new vechile(x, y, z);


    document.write("maximum speed is:" + Bike.maximumspeed);
    document.write("<br>");

    document.write("type of vehicle is:" + Bike.typesofVechile);
    document.write("<br>");

    document.write("maximum speed is:" + Bike.Displacementofvech);
    document.write("<br>");

    document.write("Quantity  of vehicle is:" + Bike.quantity);
    document.write("<br>");



    document.write(" speed is:" + Bike.speed);
</script>



